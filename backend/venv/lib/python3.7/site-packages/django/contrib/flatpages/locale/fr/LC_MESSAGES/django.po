#: venv/lib/python3.7/site-packages/django/contrib/flatpages/admin.py:12
msgid "Advanced options"
msgstr "Options avancées"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Pages statiques"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:10
#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:13
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Par exemple, « /a_propos/contact/ ». Vérifiez la présence du caractère « / » "
"en début et en fin de chaîne."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:16
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Cette valeur ne peut contenir que des lettres, des chiffres, des points, des "
"soulignés, des tirets, des barres obliques ou des tildes."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:30
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr ""
"Par exemple, « /a_propos/contact ». Vérifiez la présence du caractère « / » "
"en début de chaîne."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:43
msgid "URL is missing a leading slash."
msgstr "Le caractère « / » n'est pas présent en début de chaîne."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:48
msgid "URL is missing a trailing slash."
msgstr "Le caractère « / » n'est pas présent en fin de chaîne."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:65
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "La page statique à l’URL %(url)s existe déjà pour le site %(site)s"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "titre"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "contenu"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "autoriser les commentaires"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "nom du gabarit"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Par exemple, « flatpages/contact_page.html ». Sans définition, le système "
"utilisera « flatpages/default.html »."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "enregistrement requis"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:24
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Si coché, seuls les utilisateurs connectés auront la possibilité de voir "
"cette page."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:27
msgid "sites"
msgstr "sites"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:31
msgid "flat page"
msgstr "page statique"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:32
msgid "flat pages"
msgstr "pages statiques"
